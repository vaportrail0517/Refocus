<?xml version="1.0" encoding="utf-8"?>
<!--
  Refocus は「端末内完結」を要件とするため，Auto Backup / Device Transfer の対象を明示的に除外する．
  Android 11 以下では fullBackupContent が使われる．
-->
<full-backup-content>
    <exclude domain="database" path="." />
    <exclude domain="sharedpref" path="." />
    <exclude domain="file" path="." />
</full-backup-content>
